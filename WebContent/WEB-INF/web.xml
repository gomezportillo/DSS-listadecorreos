<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Email list</display-name>
	<welcome-file-list>
		<welcome-file>EmailListServlet</welcome-file>
	</welcome-file-list>
	<servlet>
		<description>EmailListServlet</description>
		<display-name>EmailListServlet</display-name>
		<servlet-name>EmailListServlet</servlet-name>
		<servlet-class>communication.EmailListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmailListServlet</servlet-name>
		<url-pattern>/EmailListServlet</url-pattern>
	</servlet-mapping>
</web-app>